	month = dec,
	year = {2012},
	keywords = {Agriculture, Annuals, canopy, Conception, Ecology, epidemiology, Expert knowledge, Ideotype, Life Sciences, general, model, Optimization, plant architecture, Plant Pathology, Plant Sciences, Trees},
	pages = {1--7},
	file = {Snapshot:/home/sguyader/.mozilla/firefox/9shb6xj3.default/zotero/storage/4TMQXVU3/s10658-012-0126-y.html:text/html}
