	keywords = {Beet necrotic yellow vein virus, comparative epidemiology, computation, cultivar   mixture, epidemics, invasion, landscape, management, model, nursery, persistence, Plumpox virus, population-dynamics, propagator tree, ratio, sexually-transmitted diseases, thresholds},
	pages = {239--249}
}

