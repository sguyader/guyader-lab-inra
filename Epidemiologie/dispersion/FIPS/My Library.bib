	month = sep,
	year = {2012},
	keywords = {anamorph-teleomorph linkages, barcoding, Colletotrichum, database, glomerella, host specialisation, phylogeny, species concepts, systematics},
	pages = {181--213},
	file = {Full Text PDF:/home/sguyader/.mozilla/firefox/9shb6xj3.default/zotero/storage/WDCV5VGM/Cannon et al. - 2012 - Colletotrichum – current status and future directi.pdf:application/pdf;Snapshot:/home/sguyader/.mozilla/firefox/9shb6xj3.default/zotero/storage/DVHPA4WB/181.html:text/html}
