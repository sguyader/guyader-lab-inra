	keywords = {anthracnose, Colletotrichum acutatum, Colletotrichum gloeosporioides, Colletotrichum truncatum, fungi, glomerella, Glomerella cingulata, identification, infection, ITS sequences, lentil, rdna, tissues, vegetable soybean},
