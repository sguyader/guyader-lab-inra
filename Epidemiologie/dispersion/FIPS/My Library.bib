@inproceedings{guyader_is_2004,
	address = {Rennes, France},
	title = {Is {Potato} leafroll virus ({PLRV}) a slowly evolving {RNA} virus?},
	abstract = {A study was conducted (i) to obtain a clear and precise picture of Potato leafroll virus (PLRV) genetic diversity and (ii) to assess the effect of between host transmission and intra-host multiplication on the evolution of this virus. The results confirmed that the genetic diversity of PLRV was low and suggested that the virus was subject to very strong selective constraints. Thereafter, it was hypothesized that transmission by aphids and/or multiplication in the host plants were key factors structuring virus populations. Overall, the virus is poorly variable but also revealed that some isolates have undergone major evolutionary events such as recombination or deletions in a coding region. This suggests that, in natural conditions, the virus has reached an equilibrium although it seems able to rapidly adapt when subjected to environmental changes. Analyses of PLRV population structure in various conditions confirmed that vector transmission constitutes a strong constraint limiting the diversity of the population. They also showed that, by forcing the virus to explore a new cellular environment, the genome is allowed to vary more, confirming that the host plant cellular environment is also a key parameter constraining virus population diversity. In contrast, in the course of infection of different host plant species, diversity is hardly found, at least during the first months of infection, suggesting that PLRV is subject to strong   constraints within host plants, slowing down the process of adaptation to new hosts. Alternatively, it is possible that PLRV genome has a poor intrinsic variability. All these results are used to propose some   hypotheses on PLRV evolution.},
	language = {English},
	author = {Guyader, S and Tanguy, S and Andrieu, A and Pasquer, F and Giblot-Ducray, D},
	editor = {Giblot-Ducray, D},
	year = {2004},
	keywords = {genetic recombination, phytopathogens},
	file = {ISI Web of Knowledge Record:/home/sguyader/.mozilla/firefox/9shb6xj3.default/zotero/storage/MR7WIU6J/full_record.html:text/html}
}

