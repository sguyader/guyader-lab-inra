@article{wharton_biology_2004,
	title = {The biology of {Colletotrichum} acutatum},
	volume = {61},
	abstract = {Colletotrichum acutatum is a major agricultural and fungal pathogen responsible for important economic losses in fruit trees in temperate, subtropical and tropical regions. However, there are few studies on key aspects of their biology. This is because, traditionally, many isolates of C. acutatum were identified as C. gloeosporioides. Using molecular biology techniques has enabled the distinction between them. The life cycle of C. acutatum comprises a sexual and other asexual stage, and there are still many ways to know genetic sexual phase and its relevance in the structure of the population. Colletotrichum acutatum has two types of infection strategies described in the genus Colletotrichum , intracellular hemibiotrófica and subcuticular-intramural necrotrophic, and can even set a latency period in order to cope with host defense mechanisms such as: the existence of preformed toxic compounds and phytoalexins, nutrient scarcity of the host's own tissue to meet to the energy requirements of the pathogen. Colletotrichum acutatum usually overwinters as mycelium and / or appressoria in different parts of the host. Conidia require the presence of water to be produced and their dispersion is produced by rain. Current management measures of this fungus include the use of the different resistance of cultivars, forms and crop management, chemical and biological control, and preventive strategies such as predictive models of disease. Thus, the aim of this paper is to present current knowledge on various aspects of the biology of C. acutatum and related species and includes a discussion on the progress to control this fungus.},
	journal = {Anales del Jardín Botánico de Madrid},
	author = {Wharton, P. S. and Diéguez-Uribeondo, J.},
	year = {2004},
	pages = {3--22}
}

@article{green_rapid_2000,
	title = {A rapid bioassay for screening yam germplasm for response to anthracnose.},
	volume = {40},
	issn = {0041-3291},
	abstract = {A rapid detached leaf bioassay has been developed for routine screening   of yam germplasm ( Dioscorea spp.) for response to anthracnose (   Colletotrichum gloeosporioides [ Glomerella cingulata]). Five single   spore isolates of  C. gloeosporioides representing three virulence   groupings were used to screen five  D. alata cultivars with reportedly   varying host reactions. There were significant main effects in disease   severity due to cultivar, isolate and a significant cultivar*isolate   interaction, indicating that the assay differentiates isolate virulence   and host reaction to anthracnose. A whole-plant assay showed a   significant correlation with the detached leaf technique, confirming   that the latter was an accurate indicator of cultivar susceptibility.   The bioassay uses little inoculum and space and can be carried out under   controlled conditions with selected isolates.},
	language = {English},
	number = {3},
